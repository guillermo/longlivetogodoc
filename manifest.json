{
    "name": "Long Live to godoc",
    "version": "1.0",
    "description": "A browser extension that makes pkg.go.dev to display as godoc.org",
    "manifest_version": 2,
    "content_scripts": [{
        "css": ["go.dev.css"],
        "matches": ["https://beta.pkg.go.dev/*", "https://pkg.go.dev/*"]
    }],
    "icons": { 
        "16": "16.png",
        "32": "32.png",
        "48": "48.png",
        "256": "256.png",
       "128": "128.png" 
    }


}

